{
    "name": "carlosdev/model-change-logger",
    "description": "Laravel library for recording changes (update and delete) in models.",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "CarlosDev\\ModelChangeLogger\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "CarlosDev\\ModelChangeLogger\\ModelChangeLoggerServiceProvider"
        ]
      }
    },
    "require": {
      "php": "^8.0",
      "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    }
  }
  